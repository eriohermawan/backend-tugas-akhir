{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Hafalan",
        "Tilawah",
        "Tematik"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "product_variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-variant.product-variant",
      "mappedBy": "product"
    },
    "product_variant_colors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-variant-color.product-variant-color",
      "mappedBy": "product"
    },
    "transaction_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction-item.transaction-item",
      "mappedBy": "product"
    },
    "transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "product"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "cart": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cart.cart",
      "inversedBy": "products"
    }
  }
}
